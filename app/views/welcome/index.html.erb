<h1>Thoughtbox</h1>
<h2>Login</h2>
<%= form_for :session, url: login_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email%>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Login" %>
<% end %>

<h2>Don't Have an Account?</h2>
  <%= link_to('Sign Up', new_user_path) %>
